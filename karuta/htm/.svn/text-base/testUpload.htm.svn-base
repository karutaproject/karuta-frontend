		<script src="../../other/js/jquery.fileupload.js"></script>
		<script>var url = "../../../"+serverFIL+"/resources/resource/file/3fe5e0b3-b373-11e3-861e-00215e6c3f32";</script>
	</head>
	<body>
		<div id='divfileupload'>
			<script>document.write("<input id='fileupload' type='file' name='uploadfile' data-url='"+url+"'>");</script>
			<div id='progress'><div class='bar' style='width: 0%;'></div>
		</div>
		<script>
		$(document).ready(function() {
			$('#fileupload').fileupload({
				progressall: function (e, data) {
					$("#progress").css('border','1px solid lightgrey');
					$("#divfileupload").html("<img src='../../karuta/img/ajax-loader.gif'>");
					var progress = parseInt(data.loaded / data.total * 100, 10);
					$('#progress .bar').css('width',progress + '%');
				},
				done: function (e, data,uuid) {
					var uuid = data.url.substring(data.url.lastIndexOf('/')+1);
					$("#divfileupload").html("Loaded");
				}
		    });
		});</script>
	</body>
	
</html>